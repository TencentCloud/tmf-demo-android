                    app:qmui_borderColor="@color/app_color_blue"
                    app:qmui_borderWidth="1px"
                    app:qmui_radius="4dp" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/cwx_share_file"
