                app:qmui_borderColor="@color/app_color_blue"
                app:qmui_borderWidth="1px"
                app:qmui_radius="4dp" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                android:id="@+id/alipay_share_image"
                style="@style/stat_btn"
                android:text="@string/module_share_31"
                app:qmui_backgroundColor="@color/app_color_blue_3"
                app:qmui_borderColor="@color/app_color_blue"
                app:qmui_borderWidth="1px"
                app:qmui_radius="4dp" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                android:id="@+id/alipay_share_webpage"
                style="@style/stat_btn"
                android:text="@string/module_share_32"
                app:qmui_backgroundColor="@color/app_color_blue_3"
                app:qmui_borderColor="@color/app_color_blue"
                app:qmui_borderWidth="1px"
                app:qmui_radius="4dp" />
        </LinearLayout>

        <LinearLayout
            style="@style/button_wrapper_style"
            android:orientation="vertical">

