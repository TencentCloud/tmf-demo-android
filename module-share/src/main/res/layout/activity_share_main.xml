<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="?attr/qmui_topbar_height"
    android:background="@color/qmui_config_color_white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical">

        <LinearLayout
            style="@style/button_wrapper_style"
            android:orientation="vertical">

            <TextView
                style="@style/QDCommonTitle"
                android:text="@string/module_share_25" />

            <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
                android:id="@+id/wx_share_timeline"
                android:layout_width="match_parent"
                android:layout_height="?attr/qmui_list_item_height"
                app:qmui_accessory_type="switcher"
                app:qmui_commonList_titleColor="@color/app_color_blue" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                android:id="@+id/wx_share_word"
                style="@style/stat_btn"
                android:text="@string/module_share_26"
                app:qmui_backgroundColor="@color/app_color_blue_3"
                app:qmui_borderColor="@color/app_color_blue"
                app:qmui_borderWidth="1px"
                app:qmui_radius="4dp" />

            <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
                android:id="@+id/wx_share_image_bitmap"
                android:layout_width="match_parent"
                android:layout_height="?attr/qmui_list_item_height"
                app:qmui_accessory_type="switcher"
                app:qmui_commonList_titleColor="@color/app_color_blue" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                android:id="@+id/wx_share_image"
                style="@style/stat_btn"
                android:text="@string/module_share_27"
                app:qmui_backgroundColor="@color/app_color_blue_3"
                app:qmui_borderColor="@color/app_color_blue"
                app:qmui_borderWidth="1px"
                app:qmui_radius="4dp" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                android:id="@+id/wx_share_webpage"
                style="@style/stat_btn"
                android:text="@string/module_share_28"
                app:qmui_backgroundColor="@color/app_color_blue_3"
                app:qmui_borderColor="@color/app_color_blue"
                app:qmui_borderWidth="1px"
                app:qmui_radius="4dp" />
        </LinearLayout>

        <LinearLayout
                style="@style/button_wrapper_style"
                android:orientation="vertical">

            <TextView
                    style="@style/QDCommonTitle"
                    android:text="@string/module_share_29" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/cwx_share_word"
                    style="@style/stat_btn"
                    android:text="@string/module_share_30"
                    app:qmui_backgroundColor="@color/qmui_config_color_white"
                    app:qmui_borderColor="@color/app_color_blue"
                    app:qmui_borderWidth="1px"
                    app:qmui_radius="4dp" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/cwx_share_image"
                    style="@style/stat_btn"
                    android:text="@string/module_share_31"
                    app:qmui_backgroundColor="@color/qmui_config_color_white"
                    app:qmui_borderColor="@color/app_color_blue"
                    app:qmui_borderWidth="1px"
                    app:qmui_radius="4dp" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                    android:id="@+id/cwx_share_webpage"
