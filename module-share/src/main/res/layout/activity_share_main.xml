                    android:text="@string/module_share_34"
                    app:qmui_backgroundColor="@color/qmui_config_color_white"
                    app:qmui_borderColor="@color/app_color_blue"
                    app:qmui_borderWidth="1px"
                    app:qmui_radius="4dp" />

        </LinearLayout>

        <LinearLayout
            style="@style/button_wrapper_style"
            android:orientation="vertical">

            <TextView
                style="@style/QDCommonTitle"
                android:text="@string/module_share_35" />

            <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
