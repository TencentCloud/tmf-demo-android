                android:layout_height="?attr/qmui_list_item_height"
                app:qmui_accessory_type="switcher"
                app:qmui_commonList_titleColor="@color/app_color_blue" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                android:id="@+id/weibo_share"
                style="@style/stat_btn"
                android:text="@string/module_share_38"
                app:qmui_backgroundColor="@color/app_color_blue_3"
                app:qmui_borderColor="@color/app_color_blue"
                app:qmui_borderWidth="1px"
                app:qmui_radius="4dp" />
        </LinearLayout>

        <LinearLayout
            style="@style/button_wrapper_style"
            android:orientation="vertical">

            <TextView
                style="@style/QDCommonTitle"
                android:text="@string/module_share_39" />

            <com.qmuiteam.qmui.widget.roundwidget.QMUIRoundButton
                android:id="@+id/qq_share_image"
