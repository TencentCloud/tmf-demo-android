<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingLeft="24dp"
    android:paddingTop="20dp"
    android:paddingRight="24dp"
    android:paddingBottom="20dp">

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/cb_protocal"
        android:layout_width="match_parent"
        android:layout_height="?attr/qmui_list_item_height"
        app:qmui_accessory_type="switcher"
        app:qmui_commonList_titleColor="@color/qmui_config_color_blue"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/wbcf_grey_text"/>

    <com.qmuiteam.qmui.widget.grouplist.QMUICommonListItemView
        android:id="@+id/use_fcm"
        android:layout_width="match_parent"
        android:layout_height="?attr/qmui_list_item_height"
        app:qmui_accessory_type="switcher"
        app:qmui_commonList_titleColor="@color/qmui_config_color_blue"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/wbcf_grey_text"/>
</LinearLayout>