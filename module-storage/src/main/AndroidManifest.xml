            <intent-filter>
                <action android:name="com.tencent.tmf.storage.app.kv.MyService_1" />
            </intent-filter>
        </service>

        <provider
            android:name=".kv.MultiProcessSharedPreferences"
            android:authorities=".MultiProcessSharedPreferences"
            android:exported="false"
            android:process=".MultiProcessSharedPreferences" />
        <provider
            android:name="com.tencent.mmkv.MMKVContentProvider"
            android:authorities="com.tencent.tmf.storage.app.kv.MMKVContentProvider"
            android:exported="false" />
