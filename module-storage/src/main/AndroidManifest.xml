<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tencent.tmf.module.storage">
    <application>
        <activity android:name=".StorageActivity" android:screenOrientation="portrait"/>
        <activity android:name=".StorageSelectActivity" android:screenOrientation="portrait"/>
        <activity android:name=".file.FileActivity" android:screenOrientation="portrait"/>
        <activity android:name=".db.DBActivity" android:screenOrientation="portrait"/>
        <activity android:name=".db.cruddb.CRUDActivity" android:screenOrientation="portrait"/>
        <activity android:name=".db.encryptdb.EncryptDbActivity" android:screenOrientation="portrait"/>
