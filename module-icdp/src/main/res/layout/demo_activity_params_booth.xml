<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    >

    <View
        android:layout_width="match_parent"
        android:layout_height="20dp"/>
    <include android:id="@+id/item_params_booth_broadcast" layout="@layout/title_value_item"></include>
    <include android:id="@+id/item_params_booth_banner" layout="@layout/title_value_item"></include>
    <include android:id="@+id/item_params_booth_image" layout="@layout/title_value_item"></include>
    <include android:id="@+id/item_params_booth_splash" layout="@layout/title_value_item"></include>
    <include android:id="@+id/item_params_booth_popup" layout="@layout/title_value_item"></include>
    <include android:id="@+id/item_params_booth_list" layout="@layout/title_value_item"></include>
    <include android:id="@+id/item_params_booth_hotsplash" layout="@layout/title_value_item"></include>

    <TextView
        android:id="@+id/save_params"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_submit"
        android:gravity="center"
        android:padding="8dp"
        android:text="@string/module_icdp_15"
        android:textColor="#fff"
        android:textSize="16sp"></TextView>

</LinearLayout>